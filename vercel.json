{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http://127.0.0.1:8000/api/$1",
        "methods": ["GET", "POST"]
      },
      {
        "src": "/api/docs",
        "dest": "http://127.0.0.1:8000/api/docs"
      },
      {
        "src": "/api/openapi.json",
        "dest": "http://127.0.0.1:8000/api/openapi.json"
      }
    ]
  }
