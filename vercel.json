{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http://127.0.0.1:8000/api/$1"
      },
      {
        "src": "/docs",
        "dest": "http://127.0.0.1:8000/docs"
      },
      {
        "src": "/openapi.json",
        "dest": "http://127.0.0.1:8000/openapi.json"
      }
    ]
  }
